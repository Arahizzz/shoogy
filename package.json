    "@tamagui/shorthands": "1.74.8",
    "@tamagui/themes": "1.74.8",
    "@wuba/react-native-echarts": "^1.3.0",
    "algomatic": "^3.2.1",
    "echarts": "^5.5.0",
    "expo": "~50.0.5",
    "expo-constants": "~15.4.5",
    "expo-font": "~11.10.2",
    "expo-linking": "~6.2.2",
